
<h1 id="log_in">Log In</h1>

<%= form_tag '/login' do %>

<%= link_to (image_tag 'back_arrow'), '/welcome',class: "back_arrow" %>

<%= image_tag 'wobblee_logo_cut', class: "logo_pic" %>

<a class="name">User Name</a>
<%= text_field_tag '', nil, class: "log_in_user_name" %>

<a class="password">Password</a>
<%= text_field_tag '', nil, class: "log_in_password" %>


<% end %>

<%= link_to 'Log In', '/play_game', class: "signup_button" %>
